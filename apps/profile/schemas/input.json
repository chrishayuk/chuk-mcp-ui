{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["type", "version", "points"],
  "properties": {
    "type": { "const": "profile" },
    "version": { "const": "1.0" },
    "title": { "type": "string" },
    "points": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["x", "y"],
        "properties": {
          "x": { "type": "number" },
          "y": { "type": "number" }
        }
      }
    },
    "xLabel": { "type": "string" },
    "yLabel": { "type": "string" },
    "fill": { "type": "boolean" },
    "color": { "type": "string" },
    "markers": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["x", "label"],
        "properties": {
          "x": { "type": "number" },
          "label": { "type": "string" },
          "color": { "type": "string" },
          "icon": { "type": "string" }
        }
      }
    }
  }
}
